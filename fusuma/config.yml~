swipe:
  3: 
    left: 
      shortcut: 'alt+Tab'
    right: 
      shortcut: 'alt+Tab+Right'
    up: 
      shortcut: 'ctrl+t'
    down: 
      shortcut: 'ctrl+super+d'
  4:
    left: 
      shortcut: 'ctrl+alt+Left'
    right: 
      shortcut: 'ctrl+alt+Right'
    up: 
      shortcut: 'super+a'
    down: 
      shortcut: 'super+s'
